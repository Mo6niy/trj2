pipeline {
    agent any
        parameters {
        string(name: 'NAME', defaultValue: 'AAAAA', description: 'Your first name')
        string(name: 'SCOPE', defaultValue: '33333', description: 'scope of cluster')
        string(name: 'REGION', defaultValue: 'WWWWWWW', description: 'location')
    }
       
    stages {
        stage('Clean Workspace') {
            steps {
                cleanWs()
            }
        }   
        stage('parameters') {
            steps {
                sh """
                echo "This is additional test parrameter  ${params}"
                echo "Bye ${params.NAME}"
                """
            }
        }
    }
}


